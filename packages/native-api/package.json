{
    "name": "@tsereact/node-rust-windows-native-api",
    "version": "1.0.0",
    "type": "module",
    "devDependencies": {
        "@rollup/plugin-typescript": "^10.0.1",
        "@types/jest": "^29.2.4",
        "@types/node": "^18.11.11",
        "jest": "^29.3.1",
        "rollup": "^3.6.0",
        "ts-jest": "^29.0.3",
        "typescript": "^4.9.3"
    },
    "scripts": {
        "build": "rollup -c rollup.config.mjs",
        "watch": "rollup -c rollup.config.mjs --watch",
        "test": "jest"
    },
    "exports": {
        "./*/index": {
            "require": "./dist/cjs/*.cjs",
            "import": "./dist/esm/*.mjs",
            "default": "./dist/esm/*.mjs"
        },
        "./*": {
            "require": "./dist/cjs/*.cjs",
            "import": "./dist/esm/*.mjs",
            "default": "./dist/esm/*.mjs"
        }
    },
    "typesVersions": {
        "*": {
            "src/*": [
                "src/*.ts",
                "src/*/index.ts"
            ],
            "*": [
                "dist/dts/*.d.ts",
                "dist/dts/*/index.d.ts",
                "src/*.ts",
                "src/*/index.ts"
            ]
        }
    },
    "dependencies": {
        "tslib": "^2.4.1"
    }
}
